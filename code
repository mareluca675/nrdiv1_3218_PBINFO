#include <bits/stdc++.h>

using namespace std;

long long n, v[500000], x[51], i, j, k = 0, d, numardivizori = 1, exponent, maxi = -1;
char prim[3165001];

int main()
{
    ifstream fin("nrdiv.in");
    ofstream fout("nrdiv.out");
    
    fin >> n;
    
    for(i = 1; i <= n; i ++)
    {
        fin >> x[i]; 
        
        if(x[i] > maxi)
        {
            maxi = x[i];
        }
    }
    
    int y = sqrt(maxi);
    
    for(i = 2; i <= y; i ++)
    {
        if(prim[i] == 0)
        {
            
        k ++;
        v[k] = i;
            
            for(j = 2; i * j <= y; j ++)
            {
                prim[i * j] = 1;
            }
            
        }
    }
    
    for(i = 1; i <= n; i ++)
    {
        numardivizori = 1;
        exponent = 0;
       
        for(d = 1; d <= k; d ++)
        {
            while(x[i] % v[d] == 0)
            {
                exponent ++;
                x[i] = x[i] / v[d];
            }
            
            numardivizori = numardivizori * (exponent + 1);
            exponent = 0;
        }
        
        if(x[i] != 1)
        {
            numardivizori = numardivizori * 2;
        }
        
        fout << numardivizori << '\n';
    }

    return 0;
}
